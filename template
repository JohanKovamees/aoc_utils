use anyhow::{Context, Result};
use aoc_utils::prelude::*;

fn main() -> Result<()> {
    /// Depending on input, use other funcs from lib
    let input = read_to_string("inputs/input.txt").context("reading input")?;
    let values: Vec<i32> = parse_lines(&input).context("parsing input as i32")?;

    let part1 = solve_part1(&values)?;
    println!("Part 1: {part1}");

    let part2 = solve_part2(&values)?;
    println!("Part 2: {part2}");

    Ok(())
}

fn solve_part1(depths: &[i32]) -> Result<usize> {
    let count = 0;

    Ok(count)
}

fn solve_part2(depths: &[i32]) -> Result<usize> {
    let count = 0;

    Ok(count)
}
